<%= simple_form_for restaurant do |f| %>
  <%= f.label :name, "Name" %>
  <%= f.text_field :name %>
  <%= f.label :address, "Address" %>
  <%= f.text_area :address %>
  <%= f.label :category, "Category" %>
  <%= f.text_area :category %>
  <%= f.label :phone_nubmer, "Phone Number" %>
  <%= f.number_field :phone_number %>
  <%= f.submit %>
<% end %>
